<ivysettings>
	<settings defaultResolver="default" />

	<resolvers>
		<ibiblio name="public" m2compatible="true" />
		<ibiblio name="revere" m2compatible="true"
			root="http://www.smartclient.com/maven2/" />

		<chain name="default" returnFirst="true">
			<resolver ref="revere" />
			<resolver ref="public" />
		</chain>

	</resolvers>
</ivysettings>
